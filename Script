--[[function autoTap()
while getgenv().autoTap == true do wait()
    for i,v in pairs(game:GetService(pXTEhxnaskDE('uKEYkAmFQfWOBwNIoEaDGYcpIiBKUrduoejVnqmlVPWgErUdXxlDNZPV29ya3NwYWNl')).Plots.VIPawesomeness777.Blocks:GetChildren()) do
    

game:GetService(pXTEhxnaskDE('vewxBdPUKGoYGOjUWPzhsSUygKkrpsTUnZLJIuZrUzHsblAIqPRzUWeUmVwbGljYXRlZFN0b3JhZ2U=')).Functions.Tap:FireServer(workspace.Plots.VIPawesomeness777.Blocks[v.Name])
            
    end
