function autoMerge()
    while getgenv().autoMerge == true do wait()
    local v_h = {
        players = game.Players.LocalPlayer,
        plot = workspace.Plots
    }
    
    for _,v in next, v_h.plot:GetChildren() do
        if v.Name == v_h.players.Name then
            for _,inside in next, v.Blocks:GetChildren() do
               inside.CFrame = game:GetService(pXTEhxnaskDE('PNWBHqfHIbpJYdzfTJmnubwwnpADWKNesGGlIneThVFcsVCjZltnOuwV29ya3NwYWNl')).Plots.VIPawesomeness777.Main.CFrame
               inside.CanCollide = false
            end
        end
    end
end
end

function autoMerge2()
    while getgenv().autoMerge2 == true do wait()
local v_h = {
    players = game.Players.LocalPlayer,
    plot = workspace.Plots
}

for _,v in next, v_h.plot:GetChildren() do
    if v.Name == v_h.players.Name then
        for _,inside in next, v.Blocks:GetChildren() do
           inside.CFrame = v_h.players.Character.Head.CFrame
           inside.CanCollide = false
        end
    end
end
end
end

function buyMinion()
    while getgenv().buyMinion == true do wait()
game:GetService(pXTEhxnaskDE('LStBEgtuhNbriDELMfeYbkclXYhvZBOYhNCGinxbcBlJXuGXYTfhdrCUmVwbGljYXRlZFN0b3JhZ2U=')).Functions.BuyUpgrade:FireServer(getgenv().selectMinion)

    end
end


function buyUp1()
    while getgenv().buyUp1 == true do wait()

game:GetService(pXTEhxnaskDE('durPfWrZBbOxwDVpNPzvbVhlWPdYIIzsmavgLyigCIcxmSWfcpmbZJJUmVwbGljYXRlZFN0b3JhZ2U=')).Functions.BuyUpgrade:FireServer(getgenv().selectUp)

    end

end

function autoFrenzy()
    while getgenv().autoFrenzy == true do wait(5)
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(266.842957, 72.2313156, 3.0860796, 1, 0, 0, 0, 1, 0, 0, 0, 1)

    end
end


function autoFast()
    while getgenv().autoFast == true do wait()
    firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, workspace.Obby.Finish, 0)
    firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, workspace.Obby.Finish, 1)
end
end

function autoBirth()
    while getgenv().autoBirth == true do wait()

game:GetService(pXTEhxnaskDE('WkTIbCxnxfsEttCrWjuDyCRhWaLyyYMNprBwcwelXjvygDRjHqvLeBTUmVwbGljYXRlZFN0b3JhZ2U=')).Functions.Rebirth:InvokeServer()
    end
end

function buyRebirth()
    while getgenv().buyRebirth == true do wait() 

game:GetService(pXTEhxnaskDE('EWoOJikzBoVJhtXCTkffDjyMBrzRsvpoJsfBityOSsHMmNagmBFsymnUmVwbGljYXRlZFN0b3JhZ2U=')).Functions.GemUpgrade:FireServer(getgenv().selectRebirth)
    end

end

